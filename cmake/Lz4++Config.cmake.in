@PACKAGE_INIT@  # CMake自动注入的初始化代码

set(Lz4++_INCLUDE_DIRS "@Lz4++_INSTALL_PREFIX@/include")
set(Lz4++_BIN_PATH "@Lz4++_INSTALL_PREFIX@/bin")
set(Lz4++_LIB_PATH "@Lz4++_INSTALL_PREFIX@/lib")
set(ENABLED_STATIC_LZ4XX @BUILD_STATIC_LZ4XX@)

if(${CMAKE_BUILD_TYPE} STREQUAL "Debug")
    set(BUILD_TYPE DEBUG)
else()
    set(BUILD_TYPE RELEASE)
endif()
include("${CMAKE_CURRENT_LIST_DIR}/Lz4++Targets.cmake")
unset(BUILD_TYPE)
